#!/usr/bin/env node

'use strict'

var StompServer = require('../lib/stomp/server')
var settings = require('../settings.js')
var fs = require('fs')

fs.readFile(settings.servicesFile, 'utf8', function (err, data) {
  if (err) throw err

  var services = JSON.parse(data)
  var server = new StompServer(settings, services)
  server.start()
})
